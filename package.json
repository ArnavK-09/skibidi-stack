{
	"name": "skibidi-stack",
	"module": "index.ts",
	"type": "module",
	"author": "ArnavK-09 <arnavkaushal09@gmail.com>",
	"devDependencies": {
		"@arnavk-09/unwrap-go": "^0.1.1",
		"@biomejs/biome": "1.9.4",
		"@clack/prompts": "^0.10.0",
		"@types/bun": "latest",
		"figlet": "^1.8.0",
		"handlebars": "^4.7.8",
		"kleur": "^4.1.5",
		"tsdot": "^1.1.7",
		"tsup": "^8.4.0"
	},
	"peerDependencies": {
		"typescript": "^5.8.2"
	},
	"scripts": {
		"fmt": "biome format --fix .",
		"fmt:check": "biome format .",
		"lint": "biome lint --unsafe --fix .",
		"lint:check": "biome lint .",
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"build": "bun build ./index.ts --target bun --outfile dist/index.js",
		"pre": "bun run fmt && bun run lint && bun run check:fix"
	},
